

<h2>Tickets</h2>
@{
    string path = "data:image/jpeg;base64,";
}
<form asp-action="Tickets" method="post">
    
    
    @for (var i = 1; i < 36; i++)
    {
        <div class="col-md-3 mb-2">
                <div class="card">
                    <img class="img-thumbnail" src="@path"/>
                    <div class="card-body">
                        <h5 class="card-title mb-3">
                            Ticket @i
                        </h5>
                        <p class="card-text">
                            <button asp-route-id="@i" type="submit" >Go to Ticket</button>
                        </p>
                    </div>
                </div>
        </div>
    }

</form>





<style>
    a {
        text-decoration: none;
    }

        a:hover .card {
            box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2), 0 6px 20px 0 rgba(0, 0, 0, 0.19);
            transition: box-shadow 0.3s ease-in-out;
        }

    .card {
        transition: box-shadow 0.3s ease-in-out;
        overflow: hidden;
    }

    a:hover img {
        transform: scale(1.07);
        transition: 0.5s ease-in-out;
    }

    img {
        transition: 0.5s ease-in-out;
    }
}
</style>